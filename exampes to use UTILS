declare
  i pls_integer;
  j pls_integer;
begin
  xl.open_log('DG_TEST','Dima - test');
  for i in 1..9 loop
    XL.begin_action('Calculating row');
    for j in 1..9 loop
      xl.begin_action('Calculationg cell');
      xl.end_action('Result='||i*j);
    end loop;
    xl.end_action('Done');
  end loop;
  
  xl.close_log('Successfully completed');
end;
/

exec xl.close_log('Successfully completed');

